<% provide(:title, 'Create a team') %>
<h1>Create a team</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@team) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Create a team", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>